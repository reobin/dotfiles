[core]
	editor = nvim
	excludesfile = /Users/reobin/.gitignore_global

[alias]
	cut = !git remote prune origin
	b = !gitin branch
	br = branch
	d = difftool
	p = push
	c = commit
	co = checkout
	pl = pull
	s = !gitin status
	l = !gitin log

[diff]
	tool = vimdiff
	algorithm = patience

